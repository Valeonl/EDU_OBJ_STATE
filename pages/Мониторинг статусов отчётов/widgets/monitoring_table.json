      "onClick": "{{(async () => {\n\ttry {\n\t  const directory_inn_id = currentRow[\"directory_inn_id\"];\n\t\tconst status = currentRow[\"status_system_name\"];\n\t\tconsole.log(directory_inn_id)\n\t\t// Выполняем запрос для получения пользователя\n\t\tconst [user] = await report_review_inn.run({\n\t\t\t\"directory_inn_id\": directory_inn_id\n\t\t}).catch((error) => {\n\t\t\tthrow new Error(`Ошибка при выполнении запроса: ${error.message}`);\n\t\t});\n\n\t\t// Проверяем, получен ли пользователь\n\t\tif (!user) {\n\t\t\tthrow new Error(\"Пользователь не найден\");\n\t\t}\n\n\t\t// Проверяем значение в столбце статус\n\t\tif (status === 'never_login') {\n\t\t\tconsole.log('Заполнить за ОУ');\n\t\t\t\n\t\t\t// Если статус true, инициализируем первую запись статуса в report_status_history\n\t\t\tawait login_user_init_first_status.run({\n\t\t\t\tsession_id: appsmith.store.user_session.user_session_id, // ID сессии\n\t\t\t\tou_id: user.ou_id,          // ИНН учреждения\n\t\t\t\treport_system_name: 'edu_object_state' // Системное имя отчета\n\t\t\t});\n\t\t}\n\t\t\n\t\t// Сохраняем пользователя в store\n      await storeValue(\"user\", user);\n\n      // Перенаправляем на страницу \"Отчёт\"\n      navigateTo('Отчёт');\n\t} catch (error) {\n\t\tconsole.error(\"Произошла ошибка:\", error);\n\t\tshowAlert(error.message, \"error\"); // Показываем сообщение об ошибке\n\t}\n})();\n}}",
