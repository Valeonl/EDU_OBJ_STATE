      "onClick": "{{\n  (async () => {\n    try {\n\t  const directory_inn_id = currentRow[\"directory_inn_id\"];\n\t\t// Выполняем запрос для получения пользователя\n\t\tconst [user] = await report_review_inn.run({\n\t\t\t\"directory_inn_id\": directory_inn_id\n\t\t}).catch((error) => {\n\t\t\tthrow new Error(`Ошибка при выполнении запроса: ${error.message}`);\n\t\t});\n\n      // Проверяем, получен ли пользователь\n      if (!user) {\n        throw new Error(\"Пользователь не найден\");\n      }\n\n      // Выполняем запрос under_iogv_review\n      await under_iogv_review.run({\n        directory_inn_id: user.ou_id, comment: 'Начал проверку отчёта'}).catch((error) => {\n        throw new Error(`Ошибка при выполнении запроса under_iogv_review: ${error.message}`);\n      });\n\n      // Сохраняем пользователя в store\n      await storeValue(\"user\", user);\n\n      // Перенаправляем на страницу \"Отчёт\"\n      navigateTo('Отчёт');\n    } catch (error) {\n      console.error(\"Произошла ошибка:\", error);\n      showAlert(error.message, \"error\"); // Показываем сообщение об ошибке\n    }\n  })();\n}}",
